@model IEnumerable<MsiShopFinal.Models.NOrders>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResellerId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProdSerialNumber)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResellerId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProdSerialNumber)
        </td>
    </tr>
}

</table>
